  <!-- Hero Section -->
  <section id="home" class="hero">
    <div class="overlay"></div>
    <div class="container text-center">
      <div class="hero-content">
        <h1 class="display-4 fw-bold mb-4">Find the Perfect Gated Community for You</h1>
        <p class="lead mb-5">Experience premium living with enhanced security, exclusive amenities, and a vibrant
          community lifestyle.</p>
        <div class="d-flex justify-content-center gap-3 flex-wrap">
          <a href="#Registered_Commmunities" class="btn btn-primary btn-lg">Explore Communities</a>
         
        </div>
      </div>
    </div>
  </section>

  <section id="benefits" class="py-5">
    <div class="container">
      <div class="text-center mb-5">
        <h2 class="display-5 fw-bold">Why Choose a Gated Community?</h2>
        <p class="lead text-muted">Discover the advantages of exclusive community living</p>
      </div>

      <div class="row g-4">
        <div class="col-12 col-sm-6 col-lg-3">
          <div class="card h-100 border-0 shadow-sm">
            <div class="card-body text-center p-4">
              <div class="feature-icon mb-3">
                <i class="fas fa-shield-alt fa-3x text-primary"></i>
              </div>
              <h3 class="h4 mb-3">Enhanced Security</h3>
              <p class="text-muted">24/7 security personnel, surveillance systems, and controlled access points ensure
                your family's safety.</p>
            </div>
          </div>
        </div>

        <div class="col-12 col-sm-6 col-lg-3">
          <div class="card h-100 border-0 shadow-sm">
            <div class="card-body text-center p-4">
              <div class="feature-icon mb-3">
                <i class="fas fa-swimming-pool fa-3x text-primary"></i>
              </div>
              <h3 class="h4 mb-3">Premium Amenities</h3>
              <p class="text-muted">Enjoy exclusive access to swimming pools, fitness centers, parks, and recreational
                facilities.</p>
            </div>
          </div>
        </div>

        <div class="col-12 col-sm-6 col-lg-3">
          <div class="card h-100 border-0 shadow-sm">
            <div class="card-body text-center p-4">
              <div class="feature-icon mb-3">
                <i class="fas fa-users fa-3x text-primary"></i>
              </div>
              <h3 class="h4 mb-3">Community Living</h3>
              <p class="text-muted">Build meaningful connections with neighbors who share similar values and lifestyles.
              </p>
            </div>
          </div>
        </div>

        <div class="col-12 col-sm-6 col-lg-3">
          <div class="card h-100 border-0 shadow-sm">
            <div class="card-body text-center p-4">
              <div class="feature-icon mb-3">
                <i class="fas fa-bolt fa-3x text-primary"></i>
              </div>
              <h3 class="h4 mb-3">Seamless Utilities</h3>
              <p class="text-muted">Enjoy uninterrupted power, a reliable water supply, and well-maintained
                infrastructure for a stress-free living experience.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>


  <div class="container-fluid px-5">
    <div class="row justify-content-center">
      <div class="col-lg-10">
        <div class="community card shadow">
          <div class="card-body d-flex flex-column flex-lg-row align-items-center justify-content-between p-4">
            <h3 class="text-center text-lg-start mb-3 mb-lg-0 fw-bold">Registered Communities</h3>
           
          </div>



          <div id="communityCarousel" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
              <% for (let i=0; i < communities.length; i +=3) { %>
                <div class="carousel-item <%= i === 0 ? 'active' : '' %>">
                  <div class="row d-flex justify-content-center gx-4 gy-4"> <!-- Increased horizontal spacing -->
                    <% for (let j=i; j < i + 3 && j < communities.length; j++) { %>
                      <div class="col-12 col-md-6 col-lg-4">
                        <div class="card community-card h-100 border-0 shadow-sm p-3"> <!-- Padding inside card -->
                          <img src="<%= communities[j].image %>" class="card-img-top" alt="<%= communities[j].name %>">
                          <div class="card-body p-4">
                            <h3 class="h4 mb-2">
                              <%= communities[j].name %>
                            </h3> <!-- Added margin bottom -->

                            <!-- Updated Amenities Section -->
                            <div class="d-flex flex-wrap justify-content-start mb-3 gap-2">
                              <% if (Array.isArray(communities[j].amenities)) { %>
                                <% communities[j].amenities.forEach(amenity=> { %>
                                  <span class="badge bg-primary px-3 py-2 fs-6 text-truncate">
                                    <%= amenity %>
                                  </span>
                                  <% }); %>
                                    <% } else { %>
                                      <span class="badge bg-primary px-3 py-2 fs-6 text-truncate">
                                        <%= communities[j].amenities %>
                                      </span>
                                      <% } %>
                            </div>

                            <p class="text-muted mb-3">
                              <i class="fas fa-map-marker-alt me-2"></i>
                              <%= communities[j].location %>
                            </p>

                            <p class="card-text">
                              <%= communities[j].description %>
                            </p>
                          </div>
                        </div>
                      </div>
                      <% } %>
                  </div>
                </div>
                <% } %>
            </div>
          </div>



          <!-- Navigation Arrows -->
          <button class="carousel-control-prev custom-arrow" type="button" data-bs-target="#communityCarousel"
            data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          </button>
          <button class="carousel-control-next custom-arrow" type="button" data-bs-target="#communityCarousel"
            data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
          </button>

        </div>
      </div>
    </div>
  </div>
  



  >

  <!-- Services & Features -->
